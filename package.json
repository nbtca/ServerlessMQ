		"@types/service-worker-mock": "^2.0.4",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.6",
		"wrangler": "^4.33.1"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"pnpx @biomejs/biome format --write ./src --no-errors-on-unmatched --staged"
